print("input word: ")
word = input()

def converttopiglatin(sentence):
    words = sentence.split()
    text = []
    for word in words:
        pigword = word[1:] + word[0] + "ay"
        text.append(pigword)
    return " ".join(text)

if word[0] == "a" or word[0] == "e" or word[0] == "i" or word[0] == "o" or word[0] == "u":
    print(word + "way")
else:
    print(converttopiglatin(word))